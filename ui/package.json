{
  "name": "grand-stack-starter-ui",
  "version": "0.0.1",
  "description": "UI app for GRANDstack",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "now-build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "apollo-boost": "^0.4.4",
    "apollo-cache": "^1.3.2",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-utilities": "^1.3.2",
    "classnames": "^2.2.6",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-apollo": "^3.1.2",
    "react-dom": "^16.10.2",
    "react-scripts": "^3.2.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.5",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "typescript": "^3.6.4"
  }
}
